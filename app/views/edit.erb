<h2><%= @article.title %></h2>
<h2><%= @article.content %></h2>
<form method="post" action=<%="/articles/#{@article.id}"%>>
    <input id="hidden" type="hidden" name="_method" value="PATCH">
    <p>Title: <input type="text" name="title"></p>
    <p>Content: <input type="text" name="content"></p>
    <input type="submit">
</form>
